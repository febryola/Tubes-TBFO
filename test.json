{
  "start": "E",
  "groups": {
    "number": "(0-9)(0-9)*",
    "lower": "(a-z)(a-z)*",
    "upper": "(A-Z)(A-Z)*"
  },
  "terminals": ["(", ")", "*", "+"],
  "rules": {
    "I": [["lower"], ["I", "lower"], ["I", "number"]],
    "F": [["I"], ["(", "E", ")"]],
    "T": [["F"], ["T", "*", "F"]],
    "E": [["T"], ["E", "+", "T"]]
  }
}
